<node args="Rev4_position_controller Rev5_position_controller Rev6_position_controller Rev7_position_controller joint_state_controller " name="controller_spawner" ns="Capstone" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
